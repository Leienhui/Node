    <!-- 标准语法 -->
{{if age>20}}
    条件是年龄大于的显示20，显示模板对象的内容name={{name}}
   {{else if age<15}} 
   条件判断年龄小于15的输出  {{name}}
   {{else}}
   条件不满足走else  
{{/if}}
    <!-- 原始语法 -->
    <% if (age>20){%>
        条件是年龄大于的显示20
    <%}else if(age<12){%>
        条件是年龄小于的显示12
    <%}else{%>
        不符合条件
    <%}%>




